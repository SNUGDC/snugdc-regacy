{% extends "base/base.djhtml" %}
{% block page_id%}game-item{% endblock %}
{% block content %}
<div class="container">
    <div class="row">
        <h1>{{ game.name }}</h1>
        <div class="">
            <img src="{{ game.image.url }}" />
        </div>
        <div>
            <p>
                {{ game.description }}
            </p>
        </div>
    </div>
</div>
{% endblock %}
